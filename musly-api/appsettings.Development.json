{
  "ConnectionStrings": {
    "Redis": "localhost:6379"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "musly": {
    "library": "/usr/local/bin/musly",
    "collection": "/home/music/collection.musly",
    "jukebox": "/home/music/jukebox",
    "enableLocalCollection": true,
    "MixtapesStorage": "http://cmtz.nyc3.cdn.digitaloceanspaces.com/UploadedFiles/Mixtapes",
    "AlbumsStorage": "http://cmtz.nyc3.cdn.digitaloceanspaces.com/UploadedFiles/Albums"
  }
}
